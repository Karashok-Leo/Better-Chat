{
	"search.exclude": {
		"/node_modules": true,
		"dist": true
	},
	"editor.formatOnSave": true,
	"eslint.execArgv": null,
	"eslint.codeActionsOnSave.rules": null,
	"eslint.format.enable": true,
	"eslint.validate": [],
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
